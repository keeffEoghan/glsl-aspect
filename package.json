{
  "name": "glsl-aspect",
  "version": "1.0.0",
  "description": "Small GLSL utility for aspect ratios in 1-4 dimensions.",
  "main": "index.glsl",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keeffEoghan/glsl-aspect"
  },
  "author": "Eoghan O'Keeffe <epok.tech@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10.16.3",
    "yarn": "1.19.0"
  },
  "scripts": {
